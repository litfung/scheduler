{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "ritwik310/scheduler-client",
			"hostname": "client",
			"essential": false,
			"memory": 128
		},
		{
			"name": "server",
			"image": "ritwik310/scheduler-server",
			"hostname": "server",
			"essential": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "ritwik310/scheduler-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": [ "client", "server" ],
			"memory": 128
		}
	]
}
